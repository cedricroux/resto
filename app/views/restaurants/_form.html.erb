<%= simple_form_for @restaurant, :html => { :multipart => true } do |f| %>
  <%= select_tag(:type_id, options_for_select(@types), :prompt => "Select a type") %>
  <%= f.input :name, label: "Restaurant Name" %>
  <%= f.input :description %>
  <%= f.input :address %>
  <%= f.button :submit %>
<% end %>
